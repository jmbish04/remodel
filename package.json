{
  "name": "ultimate-ai-architect",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "pnpm run db:generate && wrangler d1 migrations apply DB --local",
    "db:migrate:remote": "pnpm run db:generate && wrangler d1 migrations apply DB --remote",
    "worker:dev": "wrangler dev",
    "deploy": "pnpm run build && pnpm run db:migrate:remote && npx wrangler deploy",
    "docker:local": "pnpm run worker:dev && pnpm run docker:clear-local-port && docker build -t remodel-container . && docker run -p 3000:3000 --env-file .dev.vars -e PORT=3000 remodel-container",
    "docker:clear-local-port": "lsof -ti :3000 | xargs kill -9 || true"
  },
  "dependencies": {
    "@google/genai": "^1.37.0",
    "@mapbox/react-range": "^0.0.7",
    "area-polygon": "^1.0.1",
    "better-sqlite3": "^12.6.2",
    "convert-units": "^2.3.4",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.4",
    "immutable": "^5.1.4",
    "immutablediff": "^0.4.4",
    "immutablepatch": "^0.5.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "next": "^15.5.9",
    "polylabel": "^2.0.1",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-svg-pan-zoom": "^3.13.1",
    "react-tabs": "^6.1.0",
    "redux": "^5.0.1",
    "shortid": "^2.2.17",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@cloudflare/containers": "^0.1.0",
    "@cloudflare/workers-types": "^4.20260117.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.7",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "drizzle-kit": "^0.31.8",
    "happy-dom": "^20.3.1",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
